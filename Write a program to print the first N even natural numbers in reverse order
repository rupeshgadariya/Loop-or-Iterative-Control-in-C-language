// Write a program to print the first N even natural numbers in reverse order 

 #include<stdio.h>
#include<conio.h>

    void main()
{
        int x;
        printf("Enter a number ");
        scanf("%d",&x);
        x=2*x;
        while (x>=1)
  {  if (x%2==0)
        printf("\n%lu",x);
        x--;
  }   
    getch();
}
