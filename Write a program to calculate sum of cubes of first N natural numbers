// Write a program to calculate sum of cubes of first N natural numbers

#include<stdio.h>
#include<conio.h>

    void main()
{
          int a=1,x,sum=0;
        printf("Enter a number ");
        scanf("%d",&x);
        while (a<=x)
        {   sum = sum + ( a*a*a );
           a++;
        }
           printf("Sum of squares of first %d natural number = %lu",x,sum);
    
        
    getch();
}
