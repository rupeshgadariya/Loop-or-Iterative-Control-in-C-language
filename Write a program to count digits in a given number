// Write a program to count digits in a given number 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x,count=0;
        printf("Enter the number ");
        scanf("%d",&x);

    while (x)
    {
        x = x/10;
        count++;
    }
        printf("%d Digit ",count);
        
    getch();
}
