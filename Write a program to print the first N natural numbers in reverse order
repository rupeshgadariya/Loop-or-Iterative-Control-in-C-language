// Write a program to print the first N natural numbers in reverse order 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x;
        printf("Enter a number ");
        scanf("%d",&x);
        while (x>=1)
        {
            printf("\n%lu",x);
            x--;
        }
        
        
    getch();
}
