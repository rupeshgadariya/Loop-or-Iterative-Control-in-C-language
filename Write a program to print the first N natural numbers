// Write a program to print the first N natural numbers. 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x,i=1;
        printf("Enter a number ");
        scanf("%d",&x);
        while (i<=x)
        {
            printf("\n%lu",i);
            i++;
        }
        
    getch();
}
